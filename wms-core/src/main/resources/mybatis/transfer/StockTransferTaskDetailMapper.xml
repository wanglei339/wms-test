<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lsh.wms.core.dao.transfer.StockTransferTaskDetailDao" >

	<resultMap id="stockTransferTaskDetailResultMap" type="StockTransferTaskDetail">
		<id property="id" column="id"/>
		<result property="taskId" column="task_id"/>
		<result property="planId" column="plan_id"/>
		<result property="detailId" column="detail_id"/>
		<result property="skuId" column="sku_id"/>
		<result property="itemId" column="item_id"/>
		<result property="fromLocationId" column="from_location_id"/>
		<result property="toLocationId" column="to_location_id"/>
		<result property="qty" column="qty"/>
		<result property="packName" column="pack_name"/>
		<result property="packUnit" column="pack_unit"/>
		<result property="containerId" column="container_id"/>
		<result property="createdAt" column="created_at"/>
		<result property="updatedAt" column="updated_at"/>
	</resultMap>
  
	<insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="StockTransferTaskDetail">
		insert into stock_transfer_task_detail (
			task_id,plan_id,detail_id,sku_id,item_id,from_location_id,
			to_location_id,qty,pack_name,pack_unit,container_id,created_at,
			updated_at
    	) values (
			#{taskId},#{planId},#{detailId},#{skuId},#{itemId},#{fromLocationId},
			#{toLocationId},#{qty},#{packName},#{packUnit},#{containerId},#{createdAt},
			#{updatedAt}
    	)
	</insert>

	<insert id="batchInsert" parameterType="java.util.List">
		insert into stock_transfer_task_detail (
		task_id,plan_id,detail_id,sku_id,item_id,from_location_id,
		to_location_id,qty,pack_name,pack_unit,container_id,created_at,
		updated_at
		) values
		<foreach collection="list" item="item" index="index" separator="," >
			(#{item.taskId},#{item.planId},#{item.detailId},#{item.skuId},#{item.itemId},
			#{item.fromLocationId},#{item.toLocationId},#{item.qty},#{item.packName},
			#{item.packUnit},#{item.containerId},#{item.createdAt},#{item.updatedAt})
		</foreach>
	</insert>

	<update id="update" parameterType="StockTransferTaskDetail">
		update stock_transfer_task_detail
    	<set>
  			<if test="taskId != null">task_id=#{taskId},</if>
  			<if test="planId != null">plan_id=#{planId},</if>
  			<if test="detailId != null">detail_id=#{detailId},</if>
  			<if test="skuId != null">sku_id=#{skuId},</if>
  			<if test="itemId != null">item_id=#{itemId},</if>
  			<if test="fromLocationId != null">from_location_id=#{fromLocationId},</if>
  			<if test="toLocationId != null">to_location_id=#{toLocationId},</if>
  			<if test="qty != null">qty=#{qty},</if>
  			<if test="packName != null">pack_name=#{packName},</if>
  			<if test="packUnit != null">pack_unit=#{packUnit},</if>
  			<if test="containerId != null">container_id=#{containerId},</if>
  			<if test="createdAt != null">created_at=#{createdAt},</if>
  			<if test="updatedAt != null">updated_at=#{updatedAt}</if>
		</set>
		where id=#{id}
	</update>
  
	<select id="getStockTransferTaskDetailById" parameterType="int" resultMap="stockTransferTaskDetailResultMap">
		select * from stock_transfer_task_detail
		where ID=#{id} 
	</select>
  
	<select id="countStockTransferTaskDetail" parameterType="map" resultType="int">
		select count(*) from stock_transfer_task_detail
		<include refid="queryCondition"/>
	</select>
  
	<select id="getStockTransferTaskDetailList" parameterType="map" resultMap="stockTransferTaskDetailResultMap">
		select * from stock_transfer_task_detail
		<include refid="queryCondition"/>
		<if test="start != null and limit != null">
		limit #{start},#{limit}
		</if>
	</select>
  
	<sql id="queryCondition">
		<where>
			<if test="id != null">id=#{id}</if>
			<if test="taskId != null">and task_id=#{taskId}</if>
			<if test="planId != null">and plan_id=#{planId}</if>
			<if test="detailId != null">and detail_id=#{detailId}</if>
			<if test="skuId != null">and sku_id=#{skuId}</if>
			<if test="itemId != null">and item_id=#{itemId}</if>
			<if test="fromLocationId != null">and from_location_id=#{fromLocationId}</if>
			<if test="toLocationId != null">and to_location_id=#{toLocationId}</if>
			<if test="qty != null">and qty=#{qty}</if>
			<if test="packName != null">and pack_name=#{packName}</if>
			<if test="packUnit != null">and pack_unit=#{packUnit}</if>
			<if test="containerId != null">and container_id=#{containerId}</if>
			<if test="createdAt != null">and created_at=#{createdAt}</if>
			<if test="updatedAt != null">and updated_at=#{updatedAt}</if>
		</where>
	</sql>
   
</mapper>