<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lsh.wms.core.dao.so.OutbSoDetailDao" >

	<resultMap id="outbSoDetailResultMap" type="OutbSoDetail">
		<id property="id" column="ID"/>
		<result property="detailOtherId" column="DETAIL_OTHER_ID"/>
		<result property="orderId" column="ORDER_ID"/>
		<result property="skuCode" column="SKU_CODE"/>
		<result property="itemId" column="ITEM_ID"/>
		<result property="skuId" column="SKU_ID"/>
		<result property="skuName" column="SKU_NAME"/>
		<result property="barCode" column="BAR_CODE"/>
		<result property="orderQty" column="ORDER_QTY"/>
		<result property="packUnit" column="PACK_UNIT"/>
		<result property="lotNum" column="LOT_NUM"/>
		<result property="insertby" column="INSERTBY"/>
		<result property="updateby" column="UPDATEBY"/>
		<result property="inserttime" column="INSERTTIME"/>
		<result property="updatetime" column="UPDATETIME"/>
	</resultMap>
  
	<insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="OutbSoDetail">
		insert into OUTB_SO_DETAIL (
			DETAIL_OTHER_ID,ORDER_ID,SKU_CODE,SKU_ID,SKU_NAME,BAR_CODE,ORDER_QTY,
			PACK_UNIT,LOT_NUM,INSERTBY,UPDATEBY,INSERTTIME,UPDATETIME,ITEM_ID
    	) values (
			#{detailOtherId},#{orderId},#{skuCode},#{skuId},#{skuName},#{barCode},#{orderQty},
			#{packUnit},#{lotNum},#{insertby},#{updateby},#{inserttime},#{updatetime},#{itemId}
    	)
	</insert>

	<insert id="batchInsert" parameterType="java.util.List">
		insert into OUTB_SO_DETAIL (
		DETAIL_OTHER_ID,ORDER_ID,SKU_CODE,SKU_ID,SKU_NAME,BAR_CODE,ORDER_QTY,
		PACK_UNIT,LOT_NUM,INSERTBY,UPDATEBY,INSERTTIME,UPDATETIME,ITEM_ID
		) values
		<foreach collection="list" item="item" index="index" separator="," >
			(#{item.detailOtherId},#{item.orderId},#{item.skuCode},#{item.skuId},#{item.skuName},#{item.barCode},#{item.orderQty},
			#{item.packUnit},#{item.lotNum},#{item.insertby},#{item.updateby},now(),#{item.updatetime},#{item.itemId})
		</foreach>
	</insert>


	<update id="update" parameterType="OutbSoDetail">
		update OUTB_SO_DETAIL
    	<set>
			<if test="detailOtherId != null">DETAIL_OTHER_ID=#{detailOtherId},</if>
  			<if test="orderId != null">ORDER_ID=#{orderId},</if>
  			<if test="skuCode != null">SKU_CODE=#{skuCode},</if>
  			<if test="skuId != null">SKU_ID=#{skuId},</if>
			<if test="itemId != null">ITEM_ID=#{itemId},</if>
  			<if test="skuName != null">SKU_NAME=#{skuName},</if>
  			<if test="barCode != null">BAR_CODE=#{barCode},</if>
  			<if test="orderQty != null">ORDER_QTY=#{orderQty},</if>
  			<if test="packUnit != null">PACK_UNIT=#{packUnit},</if>
  			<if test="lotNum != null">LOT_NUM=#{lotNum},</if>
  			<if test="insertby != null">INSERTBY=#{insertby},</if>
  			<if test="updateby != null">UPDATEBY=#{updateby},</if>
  			<if test="inserttime != null">INSERTTIME=#{inserttime},</if>
  			<if test="updatetime != null">UPDATETIME=#{updatetime}</if>
		</set>
		where ID=#{id}
	</update>
  
	<select id="getOutbSoDetailById" parameterType="int" resultMap="outbSoDetailResultMap">
		select * from OUTB_SO_DETAIL
		where ID=#{id} 
	</select>
  
	<select id="countOutbSoDetail" parameterType="map" resultType="int">
		select count(*) from OUTB_SO_DETAIL
		<include refid="queryCondition"/>
	</select>
  
	<select id="getOutbSoDetailList" parameterType="map" resultMap="outbSoDetailResultMap">
		select * from OUTB_SO_DETAIL
		<include refid="queryCondition"/>
		<if test="start != null and limit != null">
		limit #{start},#{limit}
		</if>
	</select>
  
	<sql id="queryCondition">
		<where>
			<if test="id != null">ID=#{id}</if>
			<if test="detailOtherId != null">and DETAIL_OTHER_ID=#{detailOtherId}</if>
			<if test="orderId != null">and ORDER_ID=#{orderId}</if>
			<if test="skuCode != null">and SKU_CODE=#{skuCode}</if>
			<if test="skuId != null">and SKU_ID=#{skuId}</if>
			<if test="itemId != null">and ITEM_ID=#{itemId}</if>
			<if test="skuName != null">and SKU_NAME=#{skuName}</if>
			<if test="barCode != null">and BAR_CODE=#{barCode}</if>
			<if test="orderQty != null">and ORDER_QTY=#{orderQty}</if>
			<if test="packUnit != null">and PACK_UNIT=#{packUnit}</if>
			<if test="lotNum != null">and LOT_NUM=#{lotNum}</if>
			<if test="insertby != null">and INSERTBY=#{insertby}</if>
			<if test="updateby != null">and UPDATEBY=#{updateby}</if>
			<if test="inserttime != null">and INSERTTIME=#{inserttime}</if>
			<if test="updatetime != null">and UPDATETIME=#{updatetime}</if>
		</where>
	</sql>
   
</mapper>